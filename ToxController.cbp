<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ToxController" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option virtualFolders="toxcontroller\;" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/ToxController" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/ToxController" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Linker>
			<Add option="-ltoxcore" />
		</Linker>
		<Unit filename="example/example.cpp" />
		<Unit filename="example/example.hpp" />
		<Unit filename="main.cpp" />
		<Unit filename="toxcontroller/toxcontroller.cpp">
			<Option virtualFolder="toxcontroller/" />
		</Unit>
		<Unit filename="toxcontroller/toxcontroller.hpp">
			<Option virtualFolder="toxcontroller/" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
